{ 
  "compilerOptions": { 
    "target": "es5", 
    "module": "commonjs", 
    "strict": true, 
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@core/*": ["src/core/*"],
      "@modules/*": ["src/modules/*"]
    }
  }, 
  "include": ["src/**/*", "tests/**/*"], 
  "exclude": ["node_modules"] 
}